{
  "kind": "collectionType",
  "collectionName": "service-message",
  "info": {
    "name": "Service message",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true
    },
    "valid_from": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime",
      "required": true
    },
    "valid_to": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime"
    },
    "message_type": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "INFO",
        "WARNING",
        "ERROR"
      ],
      "required": true
    },
    "channel": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "enumeration",
      "enum": [
        "Generell",
        "Publiseringsportal",
        "Registreringsportal",
        "Adminportal"
      ],
      "required": true
    },
    "short_description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "required": true
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "richtext"
    }
  }
}
