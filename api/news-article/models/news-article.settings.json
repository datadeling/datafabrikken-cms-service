{
  "kind": "collectionType",
  "collectionName": "news-article",
  "info": {
    "name": "News Article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "subtitle": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "published": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "datetime"
    },
    "socialImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "basic.quote",
        "basic.image",
        "basic.paragraph",
        "basic.contact",
        "basic.section",
        "basic.infobox"
      ]
    }
  }
}
